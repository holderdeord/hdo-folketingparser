      <rette_vedkommende_minister_tittel i:nil="true" />
      <sendt_dato>1996-11-15T00:00:00</sendt_dato>
      <sesjon_id>1996-97</sesjon_id>
      <sporsmal_fra>
        <versjon>1.3</versjon>
        <doedsdato>0001-01-01T00:00:00</doedsdato>
        <etternavn>Holand</etternavn>
        <foedselsdato>1946-01-20T00:00:00</foedselsdato>
        <fornavn>Lisbeth</fornavn>
        <id>LIH</id>
        <kjoenn>kvinne</kjoenn>
