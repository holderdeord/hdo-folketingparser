      </sporsmal_fra>
      <sporsmal_nummer>202</sporsmal_nummer>
      <sporsmal_til>
        <versjon>1.2</versjon>
        <doedsdato>0001-01-01T00:00:00</doedsdato>
        <etternavn>Skogsholm</etternavn>
        <foedselsdato>1959-10-18T00:00:00</foedselsdato>
