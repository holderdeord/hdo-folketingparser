      <versjon>1.2</versjon>
      <vedtak_kode>Budsjett/budsjettrelaterte</vedtak_kode>
      <vedtak_kommentar i:nil="true" />
      <vedtak_nummer>311</vedtak_nummer>
      <vedtak_referanse>(Komiteens tilråding Rnr. 20, I. )</vedtak_referanse>
      <vedtak_tekst>&lt;p&gt;På statsbudsjettet for 2012 bevilges under:&lt;/p&gt;&lt;table class="innst-table"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;Kap.&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;Post&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;Formål&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;Kroner&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;Kroner&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;Utgifter&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;2460&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;Garanti-instituttet for eksportkreditt&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;24&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;Driftsresultat: 	&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&lt;p&gt;&lt;/p&gt;&lt;/td&gt;&lt;td class="innst-cell"&gt;&l