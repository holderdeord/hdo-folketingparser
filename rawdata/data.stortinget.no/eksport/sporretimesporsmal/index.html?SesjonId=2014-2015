        <versjon>1.1</versjon>
        <doedsdato>0001-01-01T00:00:00</doedsdato>
        <etternavn>Aspaker</etternavn>
        <foedselsdato>1962-10-16T00:00:00</foedselsdato>
        <fornavn>Elisabeth</fornavn>
