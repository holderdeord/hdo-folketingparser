      <sporsmal_nummer>4</sporsmal_nummer>
      <sporsmal_til>
        <versjon>1.0</versjon>
        <doedsdato>0001-01-01T00:00:00</doedsdato>
        <etternavn>Huitfeldt</etternavn>
        <foedselsdato>1969-11-29T00:00:00</foedselsdato>
