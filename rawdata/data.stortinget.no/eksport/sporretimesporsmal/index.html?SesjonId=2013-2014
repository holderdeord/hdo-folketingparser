      <sporsmal_til>
        <versjon>1.1</versjon>
        <doedsdato>0001-01-01T00:00:00Z</doedsdato>
        <etternavn>Jensen</etternavn>
        <foedselsdato>1969-06-01T00:00:00</foedselsdato>
        <fornavn>Siv</fornavn>
        <id>SIVJ</id>
